
import (
	"std.um"
	"common.um"
	"../umbox/http/http.um"
)

fn run*(argi: int): std.Err {
	if argi != std.argc() - 1 {
		fprintf(std.stderr(), "Usage: umbox register <query>\n")
		return {}
	}
	
	name := std.argv(argi)

	resp := common.get(common.serverUrl, sprintf("api/register/%s", name))
	if !resp.ok {
		return std.error(1, resp.msg)
	}
     
	printf("%s\n", str(resp.data))

	return {}
}
