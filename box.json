{
	"name": "umbox",
	"version": "v1.4",
	"author": "Marek Maskarinec <marek@mrms.cz>",
	"license": "Unlicense/MIT",
	"description": "A package manager for Umka",
	"homepage": "https://umbox.tophat2d.dev",
	"source": "https://github.com/marekmaskarinec/umbox",
	"readme": "README.md",
	"dependencies": [
		"http",
		"filepath",
		"json",
		"jsonenc",
		"os",
		"tar",
		"strings",
		"umka",
		"args"
	],
	"include": [
	],
	"run": {
		"posix": "./umbox/umka/umka umbox.um",
		"windows": ".\umbox\umka\umka.exe umbox.um"
	},
	"build": {
		"targets": {
			"src/gitupdate.umi": {
				"sources": "src/gitupdate.c",
				"cflags": "-Wall -Iumbox/umka",
				"ldflags": "-lgit2"
			}
		}
	}
}
